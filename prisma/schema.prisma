// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

model Users {
  idUser    Int     @id @default(autoincrement())
  email String  @unique
  name  String
  middleName  String?
  surname  String
  secondSurname  String?
  birthday DateTime
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
