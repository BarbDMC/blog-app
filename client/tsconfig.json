{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "esnext"
    ],
    "baseUrl": "./",
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": true,
    "target": "esnext",
    "paths": {
      "@colors/*":["src/theme/colors/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
